<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01//EN">
<html>
    <head>
        <title>Golf</title>
        [% c.assets.export %]
    </head>
    <body>
        <h1 id="logo"><a href="[% c.uri_for('/') %]">Golfscore Tracker</a></h1>
        <ul id="menu">
            <li><a href="[% c.uri_for('/player') %]">Players</a></li>
        </ul>
        <div id="content">
            [% IF msg %]
            <p class="msg">[% msg %]</p>
            [% END %]
            [% IF err %]
            <p class="err">[% err %]</p>
            [% END %]
            [% content %]
        </div>
    </body>
</html>
